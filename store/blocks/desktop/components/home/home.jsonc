  // * Titulo vitrina 1
  "rich-text#productlist_1": {
    "props": {
      "text": "TE PUEDEN INTERESAR ✨",
      "blockClass": "text-title-slider"
    }
  },
  //* Titulo vitrina 2
  "rich-text#title-product-list-2": {
    "props": {
      "text": "DE COLOMBIA Y EL MUNDO A LA PUERTA DE TU CASA ✈️",
      "blockClass": "text-title-slider"
    }
  },
  //* Titulo vitrina 3
  "rich-text#title-product-list-3": {
    "props": {
      "text": "EQUIPA TU ESPACIO DE ESTUDIO O TRABAJO",
      "blockClass": "text-title-slider"
    }
  },
  //* Titulo Vendedores
  "rich-text#title-sellers": {
    "props": {
      "text": "**TIENDAS OFICIALES [Ver más](/)**",
      "blockClass": "text-title-slider"
    }
  },
  //* Novedades
  "flex-layout.row#desktop__banner4--novedadesrow--img1": {
    "children": [
      "flex-layout.col#global__banner--novedadescol1"
    ],
    "props": {
      "colSizing": "auto",
      "blockClass": [
        "width-85"
      ]
    }
  },
  "flex-layout.row#desktop__banner4--novedadesrow--imgs": {
    "children": [
      "flex-layout.col#global__banner--novedadescol2",
      "flex-layout.col#global__banner--novedadescol3",
      "flex-layout.col#global__banner--novedadescol4"
    ],
    "props": {
      "fullWidth": true,
      "horizontalAlign": "center",
      "colJustify": "around",
      "colSizing": "auto",
      "blockClass": [
        "desktop__banner4--novedadesrow--imgs-85"
      ]
    }
  }
}