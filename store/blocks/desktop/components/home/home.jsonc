{
  "store.home": {
    "blocks": [
      "responsive-layout.desktop",
      //* banner crm
      "responsive-layout.desktop#desktop_home-banner-crm",
      "responsive-layout.phone#phone_home-banner-crm",
      "responsive-layout.mobile#tablet_home-banner-crm",
      //* slider home 
      "responsive-layout.desktop#desktop_home_slider",
      "responsive-layout.phone#phone_home-slider",
      "responsive-layout.mobile#mobile_home-slider",

      //* Categorias
      "responsive-layout.desktop#desktop_category-home",
      "responsive-layout.phone#phone_category-home",
      "responsive-layout.mobile#mobile_category-home",

      //* Vitrina 1
      "rich-text#productlist_1",
      "flex-layout.row#product-row-list1",
      //* Banner 2
      "flex-layout.row#home-banner2-items-title",
      "flex-layout.row#home-banner2-items-group",
      //* Banner 3
      "flex-layout.row#home-banner3-items-group",
      //* Vintrina 2
      "rich-text#title-product-list-2",
      "flex-layout.row#product-row-list2",
      //* Modal
      "modal-trigger",
      //* Banner 4
      "flex-layout.row#home-banner4-items-group",
      //*Vitrina 3
      "rich-text#title-product-list-3",
      "flex-layout.row#product-row-list3",
      //* Vendedores oficiales
      "rich-text#title-sellers",
      "list-context.image-list#global__slider2--seller",
      //* Novedades
      "rich-text#global__novedades--titile",
      "flex-layout.row#desktop__banner4--novedadesrow--img1",
      "flex-layout.row#desktop__banner4--novedadesrow--imgs",
      //* Info
      "flex-layout.row#home-info",
      //* Banco Falabella
      "flex-layout.row#home-banner5",
      //* descarga la app,
      "flex-layout.row#home-banner6",
      "rich-text#info-home",
      "rich-text#info-home2"
    ]
