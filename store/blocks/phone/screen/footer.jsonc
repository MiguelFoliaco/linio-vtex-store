{
  "footer-layout.mobile#phone_footer": {
    "children": [
      "newsletter-form#mobile__footer--suscribete",
      
      "flex-layout.row#phone__footer--payment-methods",
      
      "disclosure-layout#mobile_footer--disclousure-item1",
      "disclosure-layout#mobile_footer--disclousure-item2",
      "disclosure-layout#mobile_footer--disclousure-item3",
      "flex-layout.col#mobile_footer--social-networks-col",
      "flex-layout.row#mobile_footer--social-networks",
      "flex-layout.row#mobile__footer--descarga-apps",
      "rich-text#derechos_autor"
    ],
    "props": {
      "blockClass": "global__footer"
    }
  },
  
  "custom-grid#phone__footer--payment-methods": {
    "children": [
      "flex-layout.col#global__footer--payment-metods-item1",
      "flex-layout.col#global__footer--payment-metods-item2",
      "flex-layout.col#global__footer--payment-metods-item3",
      "flex-layout.col#global__footer--payment-metods-item4",
      "flex-layout.col#global__footer--payment-metods-item5",
      "flex-layout.col#global__footer--payment-metods-item6",
      "flex-layout.col#global__footer--payment-metods-item7"
    ],
    "props": {
      "gridType": 3,
      "gap": 5,
      "width": "100%"
    }
  },
  "flex-layout.row#phone__footer--payment-methods": {
    "children": [
      "custom-grid#phone__footer--payment-methods"
    ],
    "props": {
      "horizontalAlign": "center",
      "colJustify": "between",
      "paddingTop": 2
    },
    "title": "Opciones de Pagos Mobile"
  },
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  "newsletter-form#mobile__footer--suscribete": {
    "title": "Formulario Subscribete Mobile",
    "children": [
      "rich-text#global__footer--suscribete-msg",
      "newsletter-input-email",
      "newsletter-submit#global__footer--suscribete-button",
      "newsletter-checkbox-confirmation#global__footer--suscribete-confirmation"
    ],
    "props": {
      "blockClass": "phone__footer--suscribete"
    }
  },
  
  "disclosure-layout#mobile_footer--disclousure-item1": {
    "title": "Contenido Desplegable Mobile - Acerca de Linio",
    "children": [
      "disclosure-trigger#mobile_footer--disclousure-trigger-item1",
      "disclosure-content#mobile_footer--disclousure-content-item1",
      "disclosure-state-indicator"
    ],
    "props": {
      "animated": true,
      "blockClass": "mobile_footer--disclousure"
    }
  },
  "disclosure-trigger#mobile_footer--disclousure-trigger-item1": {
    "title": "Boton de despliegue - Acerca de Linio",
    "children": [
      "rich-text#global__footer-aboutus--title"
    ],
    "props": {
      "Show": "icon#down",
      "as": "div",
      "Hide": "icon#up",
      "blockClass": "mobile_footer--disclousure-trigger"
    }
  },
  "disclosure-content#mobile_footer--disclousure-content-item1": {
    "children": [
      "link#global__footer-aboutus--pos1--item1",
      "link#global__footer-aboutus--pos1--item2",
      "link#global__footer-aboutus--pos1--item3",
      "link#global__footer-aboutus--pos1--item4",
      "link#global__footer-aboutus--pos1--item5",
      "link#global__footer-aboutus--pos1--item6",
      "link#global__footer-aboutus--pos1--item7",
      "link#global__footer-aboutus--pos1--item8",
      "link#global__footer-aboutus--pos1--item9",
      "link#global__footer-aboutus--pos1--item10",
      "link#global__footer-aboutus--pos1--item11",
      "link#global__footer-aboutus--pos1--item12",
      "link#global__footer-aboutus--pos1--item13",
      "link#global__footer-aboutus--pos1--item14",
      "link#global__footer-aboutus--pos1--item15",
      "link#global__footer-aboutus--pos1--item16"
    ],
    "title": "Contenido desplegable Acerca de linio",
    "props": {
      "blockClass": "mobile_footer--disclousure-content"
    }
  },
  
  "disclosure-trigger#mobile_footer--disclousure-trigger-item2": {
    "title": "Boton de despliegue - Servicio al Cliente",
    "children": [
      "rich-text#global__footer-aboutus--pos2--title"
    ],
    "props": {
      "Show": "icon#down",
      "as": "div",
      "Hide": "icon#up",
      "blockClass": "mobile_footer--disclousure-trigger"
    }
  },
  "disclosure-layout#mobile_footer--disclousure-item2": {
    "title": "Contenido Desplegable Mobile - Servicio al Cliente",
    "children": [
      "disclosure-trigger#mobile_footer--disclousure-trigger-item2",
      "disclosure-content#mobile_footer--disclousure-content-item2"
    ],
    "props": {
      "animated": true,
      "blockClass": "mobile_footer--disclousure"
    }
  },
  "disclosure-content#mobile_footer--disclousure-content-item2": {
    "children": [
      "link#global__footer-aboutus--pos2--item1",
      "link#global__footer-aboutus--pos2--item2",
      "link#global__footer-aboutus--pos2--item3",
      "link#global__footer-aboutus--pos2--item4",
      "link#global__footer-aboutus--pos2--item5",
      "link#global__footer-aboutus--pos2--item6",
      "link#global__footer-aboutus--pos2--item7",
      "link#global__footer-aboutus--pos2--item8",
      "link#global__footer-aboutus--pos2--item9"
    ],
    "title": "Contenido Desplegable - Servicio al Cliente",
    "props": {
      "blockClass": "mobile_footer--disclousure-content"
    }
  },
  
  "disclosure-trigger#mobile_footer--disclousure-trigger-item3": {
    "title": "Boton de despliegue - Linio Latinoamerica",
    "children": [
      "rich-text#global__footer-aboutus--pos3--title"
    ],
    "props": {
      "Show": "icon#down",
      "as": "div",
      "Hide": "icon#up",
      "blockClass": "mobile_footer--disclousure-trigger"
    }
  },
  "disclosure-layout#mobile_footer--disclousure-item3": {
    "title": "Contenido Desplegable Mobile - Linio Latinoamerica",
    "children": [
      "disclosure-trigger#mobile_footer--disclousure-trigger-item3",
      "disclosure-content#mobile_footer--disclousure-content-item3"
    ],
    "props": {
      "animated": true,
      "blockClass": "mobile_footer--disclousure"
    }
  },
  "disclosure-content#mobile_footer--disclousure-content-item3": {
    "children": [
      "link#global__footer-aboutus--pos3--item1",
      "link#global__footer-aboutus--pos3--item2"
    ],
    "title": "Contenido Desplegable Mobile - Linio Latinoamerica",
    "props": {
      "blockClass": "mobile_footer--disclousure-content"
    }
  },

  
  "icon#up": {
    "props": {
      "id": "mpa-angle--up"
    }
  },
  "icon#down": {
    "props": {
      "id": "mpa-angle--down",
      "type": "filled"
    }
  },
  
  
  
  
  
  
  "flex-layout.row#mobile_footer--social-networks": {
    "children": [
      "flex-layout.col#mobile_footer--social-networks-title-col",
      "flex-layout.col#mobile_footer--social-networks-col"   
    ],
    "props": {
      "marginTop":2,
      "colSizing": "auto",
      "horizontalAlign": "left",
      "fullWidth": true,
      "blockClass": "mobile_footer--social-networks"
    }
  },
  "flex-layout.col#mobile_footer--social-networks-col": {
    "children": ["social-networks#footer"],
    "props": {
      "width":"60%"
    }
  },
  "flex-layout.col#mobile_footer--social-networks-title-col": {
    "children": ["rich-text#mobile_footer--social-networks-title"]
  },
  "rich-text#mobile_footer--social-networks-title": {
    "props": {
      "text": "Siguenos: ",
      "props": {
        "width":"35%"
      }
    }
  },
  

  "flex-layout.row#mobile__footer--descarga-apps":{
    "children": ["image#appstore", "image#playstore"],
    "props": {
      "fullWidth": true,
      "colJustify": "between",
      "colSizing": "auto",
      "blockClass": "mobile__footer--descarga-apps"
    }
  }
}