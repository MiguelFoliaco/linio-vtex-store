    "image#about-us-content-item1-sup-items5-img": {
        "title": "Image",
        "props": {
            "src": "assets/images/icons/secure-shield.png",
            "maxHeight": "60px"
        }
    },
    "rich-text#about-us-content-item1-sup-items5-text": {
        "props": {
