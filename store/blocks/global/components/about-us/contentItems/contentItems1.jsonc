{
    "flex-layout.row#aboutus-content-items1": {
        "children": ["flex-layout.col#aboutus-content-items1"]
    },
    "flex-layout.col#aboutus-content-items1": {
        "children": [
            "rich-text#aboutus-content-title-item1",
            "flex-layout.col#aboutus-content-items1-sup"
        ],
        "props": {
            "marginTop":8
        }
    },
    "rich-text#aboutus-content-title-item1": {
        "props": {
            "text": "Que Tenemos Para Ti",
            "textAlignment": "CENTER",
            "textPosition": "CENTER",
            "blockClass": "aboutus-content-title-item1"
        }
    },
    "flex-layout.col#aboutus-content-items1-sup": {
        "children": [
            "flex-layout.row#aboutus-content-items1-sup",
            "flex-layout.col#aboutus-content-items1-bottom"
        ],
        "props": {
            "blockClass":"aboutus-content-items1-sup"
        }
    },
    "flex-layout.row#aboutus-content-items1-sup": {
        "children": [
            "flex-layout.col#aboutus-content-items1-sup-item1",
            "flex-layout.col#aboutus-content-items1-sup-item2",
            "flex-layout.col#aboutus-content-items1-sup-item3",
            "flex-layout.col#aboutus-content-items1-sup-item4"
        ],
        "props": {
            "blockClass": "aboutus-content-items1-sup_row",
            "rowGap":{
                "desktop": 3,
                "phone": 1,
                "tablet": 2
            }
        }
    },
    //* Item 1
    "flex-layout.col#aboutus-content-items1-sup-item1": {
        "children": [
            "image#about-us-content-item1-sup-items1-img",
            "rich-text#about-us-content-item1-sup-items1-text"
        ],
        "props": {
            "blockClass":"aboutus-content-items-sup"
        }
    },
    "image#about-us-content-item1-sup-items1-img": {
        "title": "Image",
        "props": {
            "src": "assets/images/icons/track.png",
            "maxHeight": "60px",
            "blockClass":"aboutus-content-items1-sup-item1"
        }
    },
    "rich-text#about-us-content-item1-sup-items1-text": {
        "props": {
            "text": "**COMPRAS SEGURAS** <br/> Tus compras son protegidas por el mismo olimpo algun cambio",
            "textAlignment": "CENTER",
            "blockClass": "aboutus-content-items-sup"
        }
    },
    //* Item 2
    "flex-layout.col#aboutus-content-items1-sup-item2": {
