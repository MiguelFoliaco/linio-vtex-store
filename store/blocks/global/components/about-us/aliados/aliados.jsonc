    "image#global_aboutus_aliados-image-4": {
        "props": {
            "src": "assets/images/marca/motorola.png",
            "maxHeight": 50
        }
    },
    "image#global_aboutus_aliados-image-5": {
        "props": {
            "src": "assets/images/marca/oster.webp",
            "maxHeight": 50
