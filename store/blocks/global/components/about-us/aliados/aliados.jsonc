{
    "flex-layout.row#global_aboutus_aliados-row": {
        "children": [
            "flex-layout.col#global_aboutus_aliados-col"
        ],
        "props": {
            "marginTop":8
        }
    },
    "flex-layout.col#global_aboutus_aliados-col":{
        "children": [
            "rich-text#global_aboutus_aliados-title",
            "flex-layout.row#custom-grid-4",
            "flex-layout.col#global_aboutus_aliados-group-link"
        ],
        "props": {
            "blockClass":""
        }
    },
    
    "rich-text#global_aboutus_aliados-title": {
        "props": {
            "text": "Aliados de la galaxia Linio",
            "blockClass": "aboutus-content-title-item1",
            "textAlignment": "CENTER",
            "textPosition": "CENTER"
        }
    },

    
    "flex-layout.row#custom-grid-4": {
        "children": [
            "custom-grid#grid-4"
        ]
    },
    "custom-grid#grid-4": {
        "props": {
            "gridType": 3,
            "gap":14,
            "marginTop":10,
            "marginBottom":40
