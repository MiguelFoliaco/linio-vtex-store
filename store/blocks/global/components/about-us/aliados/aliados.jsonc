        "children": [
            "image#global_aboutus_aliados-image-1",
            "image#global_aboutus_aliados-image-2",
            "image#global_aboutus_aliados-image-3",
            "image#global_aboutus_aliados-image-4",
            "image#global_aboutus_aliados-image-5",
            "image#global_aboutus_aliados-image-6"
        ]
    },
    "flex-layout.row#global_aboutus_aliados-group-image1": {
        "children": [
            "image#global_aboutus_aliados-image-1",
            "image#global_aboutus_aliados-image-2",
            "image#global_aboutus_aliados-image-3"
        ]
    },
    "image#global_aboutus_aliados-image-1": {
        "props": {
            "src": "assets/images/marca/hp.png",
            "maxHeight": 50
        }
    },
    "image#global_aboutus_aliados-image-2": {
        "props": {
            "src": "assets/images/marca/lenovo.png",
            "maxHeight": 50
        }
    },
    "image#global_aboutus_aliados-image-3": {
        "props": {
            "src": "assets/images/marca/mbe.png",
            "maxHeight": 50
        }
    },
    
    "flex-layout.row#global_aboutus_aliados-group-image2": {
        "children": [
            "image#global_aboutus_aliados-image-4",
            "image#global_aboutus_aliados-image-5",
            "image#global_aboutus_aliados-image-6"
