{
    "store.custom#aboutus": {
        "blocks": [
            //* Banner Desktop
            "responsive-layout.desktop#about-us-desktop-banner",
            //* Banner Mobile
            "responsive-layout.mobile#about-us-mobile-banner",
            //* Content Items 1
            "flex-layout.row#aboutus-content-items1",
            "flex-layout.row#global_aboutus_linio-numeros_row",
            "flex-layout.row#global_aboutus_aliados-row",
            "flex-layout.row#about-pdf",
            "flex-layout.row#global_about_us-feedback",
            "flex-layout.row#aboutus_descargar-app-row",
            "flex-layout.row#aboutus_time-line",
