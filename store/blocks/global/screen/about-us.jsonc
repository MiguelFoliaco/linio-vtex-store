            "flex-layout.row#global__aboutus_empieza-comprar",
            "whatsapp-button"
        ]
    },
    "flex-layout.row#about-pdf": {
        "children": [
            "pdf-reader"
        ],
        "props": {
            "marginTop": 8
        }
    },
    "pdf-reader": {
        "props": {
            "width": "80%",
